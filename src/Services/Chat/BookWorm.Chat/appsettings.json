{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 5
        }
      }
    }
  },
  "Identity": {
    "ClientId": "chat-api",
    "Scopes": {
      "chat": "Chat API"
    }
  }
}
